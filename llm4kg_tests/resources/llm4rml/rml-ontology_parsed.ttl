rdf:Alt a owl:NamedIndividual .

rdf:Bag a owl:NamedIndividual .

rdf:List a owl:NamedIndividual .

rdf:Seq a owl:NamedIndividual .

rdfs:Class a owl:Class .

<https://www.w3.org/ns/formats/Format> a owl:Class .

rdf:Literal a owl:Class .

:Encoding a owl:Class ;
    rdfs:label "Encoding class" ;
    rdfs:comment "Represents encoding formats."@en ;
    rdfs:isDefinedBy default1: .

:FunctionExecution a owl:Class ;
    rdfs:label "Function Execution" ;
    rdfs:comment "Represents a function execution."@en ;
    rdfs:isDefinedBy <http://w3id.org/rml/fnml/> .

:GatherMap a owl:Class ;
    rdfs:label "Gather Map" ;
